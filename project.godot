; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Rummager"
config/version="202501291429"
run/main_scene="res://start_menu.tscn"
config/features=PackedStringArray("4.3", "Mobile")
boot_splash/image="res://assets/boot_splash.png"
config/icon="res://assets/icon.svg"

[autoload]

Global="*res://global.gd"
Music="*res://music.tscn"

[debug]

gdscript/warnings/untyped_declaration=2

[display]

window/size/viewport_width=414
window/size/viewport_height=896
window/size/window_width_override=828
window/size/window_height_override=1792
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=6

[gui]

theme/custom_font="res://Socafe/Socafe.ttf"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
